{% extends 'base.html.twig' %}

{% block title %}QuizAttempt index{% endblock %}

{% block body %}
    <h1>QuizAttempt index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Note</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for quiz_attempt in quiz_attempts %}
            <tr>
                <td>{{ quiz_attempt.id }}</td>
                <td>{{ quiz_attempt.note }}</td>
                <td>
                    <a href="{{ path('app_quiz_attempt_show', {'id': quiz_attempt.id}) }}">show</a>
                    <a href="{{ path('app_quiz_attempt_edit', {'id': quiz_attempt.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_quiz_attempt_new') }}">Create new</a>
{% endblock %}
